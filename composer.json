{
    "name": "action-tech/codeception-testrail",
    "description": "Codeception Extension for TestRail Reporting",
    "homepage": "https://github.com/action-tech/codeception-testrail",
    "license": "MIT",
    "require": {
        "codeception/codeception": "^2.2|^3.0",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\Tests\\Unit\\": "tests/unit"
        }
    }
}
